#!/bin/bash
echo ";;;" > ./cloverlogger.conf
echo "; Config file for Gbhorwood\Cloverlogger" >> ./cloverlogger.conf
echo "; https://github.com/gbhorwood/Cloverlogger" >> ./cloverlogger.conf
echo "" >> ./cloverlogger.conf
echo "FILE=/tmp/cloverlogger" >> ./cloverlogger.conf
echo "SEPARATOR=\"::\"" >> ./cloverlogger.conf
echo "Cloverlogger config file at cloverlogger.conf"
exit(1)